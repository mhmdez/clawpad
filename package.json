{
  "name": "clawpad",
  "version": "0.3.15",
  "description": "A file-based, Notion-style document workspace for OpenClaw",
  "keywords": [
    "openclaw",
    "workspace",
    "notion",
    "markdown",
    "editor",
    "ai",
    "block-editor"
  ],
  "homepage": "https://github.com/mhmdez/clawpad#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhmdez/clawpad.git"
  },
  "license": "MIT",
  "author": "Mohamed Mostafa <mhmdez@me.com>",
  "bin": {
    "clawpad": "./bin/clawpad.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "bin/",
    "skills/",
    "openclaw-plugin/",
    ".next/standalone/",
    ".next/static/",
    "public/",
    "next.config.ts",
    "package.json"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "test": "tsx --test --test-concurrency=1 tests/**/*.test.ts",
    "test:unit": "tsx --test --test-concurrency=1 tests/unit/**/*.test.ts",
    "test:integration": "tsx --test --test-concurrency=1 tests/integration/**/*.test.ts",
    "check:installer:parity": "bash scripts/check-installer-parity.sh",
    "check:installer:smoke": "bash scripts/smoke-installer.sh",
    "check:installer:live": "bash scripts/check-live-installer.sh",
    "prepublishOnly": "next build --webpack"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/react": "^3.0.71",
    "@blocknote/core": "^0.46.2",
    "@blocknote/mantine": "^0.46.2",
    "@blocknote/react": "^0.46.2",
    "@mantine/core": "^8.3.14",
    "@sinclair/typebox": "^0.33.5",
    "@tanstack/react-query": "^5.90.20",
    "ai": "^6.0.69",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "diff": "^8.0.3",
    "framer-motion": "^12.31.0",
    "geist": "^1.5.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
